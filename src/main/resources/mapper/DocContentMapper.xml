<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.xufengtian14.badwiki.mapper.DocContentMapper">

    <select id="seleteDocContentByDocId" resultType="com.github.xufengtian14.badwiki.entity.DocContent">
        SELECT * from user
        where doc_id = #{doc_id};
    </select>

</mapper>