<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.xufengtian14.badwiki.mapper.IDocOperationTypeMapper">

    <resultMap id="docOperationTypeMap" type="com.github.xufengtian14.badwiki.model.DocOperationType">
        <id property="id" column="id"></id>
        <result property="name" column="name"></result>
        <result property="note" column="note"></result>
    </resultMap>

    <select id="selectDocOperationType" resultMap="docOperationTypeMap">
        select * from doc_operation_type
    </select>
</mapper>